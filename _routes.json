{
  "version": 1,
  "include": ["/*"],
  "exclude": [],
  "functions": {
    "/contents": {
      "function": "contents",
      "method": ["GET", "POST", "OPTIONS"]
    },
    "/contents/*": {
      "function": "contents/[id]",
      "method": ["GET", "PUT", "DELETE", "OPTIONS"]
    },
    "/images": {
      "function": "images",
      "method": ["POST", "OPTIONS"]
    },
    "/images/*": {
      "function": "images",
      "method": ["GET", "OPTIONS"]
    },
    "/_vars/*": {
      "function": "_vars/[name]",
      "method": ["GET", "OPTIONS"]
    },
    "/files": {
      "function": "files",
      "method": ["POST", "OPTIONS"]
    },
    "/files/*": {
      "function": "files/[name]",
      "method": ["GET", "OPTIONS"],
      "no_cache": true
    }
  }
} 